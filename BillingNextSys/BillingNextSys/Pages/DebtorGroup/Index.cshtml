                        </div>
                        <div>
                            <button class="btn btn-primary" id="savedalledit" type="button">Update</button>
                        </div>
                    </div>
                </form>
            </div>
            <div id="msg2"></div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>
<br /><br />

@(Html
                                                                                        .Grid(Model.DebtorGroups)
